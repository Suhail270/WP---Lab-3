<%- include('header'); -%>

    <div class="content">
        <h1>List of Clients</h1>
        <table id="catalog">
            <thead>
                <tr>
                    <th>Name</th>
                    <th scope="col">Society</th>
                    <th scope="col">Address</th>
                    <th scope="col">Zip Code</th>
                    <th scope="col">City</th>
                    <th scope="col">Phone Number</th>
                    <th scope="col">Fax Number</th>
                    <th scope="col">Confirmation</th>
                    <th scope="col">Payment Status</th>
                    <th scope="col">Additional Information</th>
                    <th scope="col">Max Outstanding</th>
                </tr>
            </thead>
            <tbody>
                <% clients.forEach(client => { %>
                    <tr>
                        <th scope="row">
                            <%= client.contact %>
                        </th>
                        <td>
                            <%= client.society%>

                        </td>
                        <td>
                            <%= client.addres %>
                        </td>
                        <td>
                            <%= client.zipcode %>
                        </td>
                        <td>
                            <%= client.city %>
                        </td>
                        <td>
                            <%= client.phone %>
                        </td>
                        <td>
                            <%= client.fax %>
                        </td>
                        <td>
                            <%= client.confirmation %>
                        </td>
                        <td>
                            <%= client.payment %>
                        </td>
                        <td>
                            <%= client.Informations %>
                        </td>
                        <td>
                            <%= client.max_outstanding %>
                        </td>
                    </tr>
                    <% }) %>
            </tbody>
        </table>
    </div>
    <br>
        
    <%- include('footer'); -%>

